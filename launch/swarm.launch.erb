<?xml version="1.0"?>

<launch>
<%
for i in 0..4
for j in 0..4%>
	<group ns="quadrotor<%=(5*i+j)%>">
		<param name="robot_description" command="cat $(find cuda_swarm)/urdf/quadrotor.xml"/>
		<node name="urdf_spawner<%=(5*i+j)%>" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
			args="-urdf -model quadrotor<%=(5*i+j)%> -param robot_description -x <%=i%> -y <%=j%>"/>
				</group>
<%end
end
%>

	<include file="$(find gazebo_ros)/launch/empty_world.launch"/>
</launch>